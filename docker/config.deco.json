{ 
  "listenAddress": ":8080",
  "account": {
    "region": "us-east-1",
    "akid": "{{ .akid }}",
    "secret": "{{ .secret }}",
    "externalId": "{{ .external_id }}",
    "role": "{{ .cross_account_role }}"
  },
  "accounts": {
    "spinup": {
      "region": "us-east-1",
      "akid": "{{ .spinup_akid }}",
      "secret": "{{ .spinup_secret }}",
      "defaultSgs": ["{{ .spinup_default_sg }}"],
      "defaultSubnets": {{ .spinup_default_subnet }},
      "defaultKmsKeyId": "{{ .spinup_default_kms_key_id }}"
    },
    "spinupsec": {
      "region": "us-east-1",
      "akid": "{{ .spinupsec_akid }}",
      "secret": "{{ .spinupsec_secret }}",
      "defaultSgs": ["{{ .spinupsec_default_sg }}"],
      "defaultSubnets": {{ .spinupsec_default_subnet }},
      "defaultKmsKeyId": "{{ .spinupsec_default_kms_key_id }}"
    }
  },
  "accountsMap": {
    "spinup": "{{ .spinup_account_number }}",
    "spinupsec": "{{ .spinupsec_account_number }}"
  },
  "flywheel": {
    "namespace": "{{ .flywheel_namespace }}",
    "redisAddress": "{{ .redis_address }}",
    "redisDatabase": "{{ .redis_database }}",
    "ttl": "{{ .flywheel_ttl }}"
  },
  "token": "{{ .api_token }}",
  "logLevel": "{{ .log_level }}",
  "org": "{{ .spinup_org }}"
}
